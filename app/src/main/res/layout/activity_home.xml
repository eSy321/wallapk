<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".HomeActivity"
        tools:ignore="MissingConstraints">

    <Button
            android:text="@string/wyloguj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/logout_button"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.691"/>
    <Button
            android:text="@string/get_list_students"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/get_students_button"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.209"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintVertical_bias="0.577"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toEndOf="@+id/get_courses_button"
            android:layout_marginStart="8dp"/>
    <android.support.v7.widget.RecyclerView
            android:layout_width="match_parent"
            android:id="@+id/get_students" tools:layout_editor_absoluteY="16dp"
            tools:layout_editor_absoluteX="0dp" android:layout_height="0dp"/>
    <Button
            android:text="Lista KursÃ³w"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/get_courses_button" android:layout_marginBottom="8dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintHorizontal_bias="0.158"
            app:layout_constraintVertical_bias="0.577" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="dodaj studenta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/create_user_page_button"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/get_students"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"/>

</android.support.constraint.ConstraintLayout>